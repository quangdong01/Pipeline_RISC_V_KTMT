o	sub x10, x12, x11 	 0100000  01011  01100  000  01010  0110011
o	addi x15, x1, -50		 111111001110   00001  000  01111  0010011
o	lw x14, 8(x2) 		 000000001000   00010  010  01110  0000011
o	sw x14, 8(x2) 		 000000001110   00010  010  01000  0100011
